{
  "name": "looker-auth",
  "version": "1.0.0",
  "description": "Looker URL signing service",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm-run-all serverBuild clientBuild",
    "build:react": "cd client && npm start",
    "build:node": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
    "build:live": "run-p build:node build:react",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node .",
    "serverBuild": "tsc -p .",
    "clientBuild": "cd client && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@looker/embed-sdk": "^1.6.1",
    "cors": "^2.8.5",
    "create-hmac": "^1.1.7",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/create-hmac": "^1.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.14",
    "@types/passport-http": "^0.3.9",
    "@types/qs": "^6.9.7",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
